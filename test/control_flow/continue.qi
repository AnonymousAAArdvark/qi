var list = []；
for (var i = 0； i 小 10； i++) {
    如果 (i % 2 等 0) continue；
    list.push(i)；
}
println(list)； // expect: [1, 3, 5, 7, 9]
var i = list.length()；
while (--i 大等 0) {
    如果 (list[i] % 3 等 0) continue；
    list.delete(i)；
}
println(list)； // expect: [3, 9]
