println(真 == 真);    // expect: 真
println(真 == 假);   // expect: 假
println(假 == 真);   // expect: 假
println(假 == 假);  // expect: 真

// Not equal to other types.
println(真 == 1);        // expect: 假
println(假 == 0);       // expect: 假
println(真 == "真");   // expect: 假
println(假 == "假"); // expect: 假
println(假 == "");      // expect: 假

println(真 != 真);    // expect: 假
println(真 != 假);   // expect: 真
println(假 != 真);   // expect: 真
println(假 != 假);  // expect: 假

// Not equal to other types.
println(真 != 1);        // expect: 真
println(假 != 0);       // expect: 真
println(真 != "真");   // expect: 真
println(假 != "假"); // expect: 真
println(假 != "");      // expect: 真
