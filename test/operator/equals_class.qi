// Bound methods have identity equality.
class Foo {}
class Bar {}

println(Foo == Foo); // expect: 真
println(Foo == Bar); // expect: 假
println(Bar == Foo); // expect: 假
println(Bar == Bar); // expect: 真

println(Foo == "Foo"); // expect: 假
println(Foo == nil);   // expect: 假
println(Foo == 123);   // expect: 假
println(Foo == 真);  // expect: 假
