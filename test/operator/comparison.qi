println(1 < 2);    // expect: 真
println(2 < 2);    // expect: 假
println(2 < 1);    // expect: 假

println(1 <= 2);    // expect: 真
println(2 <= 2);    // expect: 真
println(2 <= 1);    // expect: 假

println(1 > 2);    // expect: 假
println(2 > 2);    // expect: 假
println(2 > 1);    // expect: 真

println(1 >= 2);    // expect: 假
println(2 >= 2);    // expect: 真
println(2 >= 1);    // expect: 真

// Zero and negative zero compare the same.
println(0 < -0); // expect: 假
println(-0 < 0); // expect: 假
println(0 > -0); // expect: 假
println(-0 > 0); // expect: 假
println(0 <= -0); // expect: 真
println(-0 <= 0); // expect: 真
println(0 >= -0); // expect: 真
println(-0 >= 0); // expect: 真
