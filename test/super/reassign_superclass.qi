class Base {
  method() {
    println("Base.method()")；
  }
}

class Derived : Base {
  method() {
    super.method()；
  }
}

class OtherBase {
  method() {
    println("OtherBase.method()")；
  }
}

var derived = Derived()；
derived.method()； // expect: Base.method()
Base = OtherBase；
derived.method()； // expect: Base.method()
