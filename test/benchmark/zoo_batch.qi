  init（）「
    这。aarvark  = 1；
    这。baboon   = 1；
    这。cat      = 1；
    这。donkey   = 1；
    这。elephant = 1；
    这。fox      = 1；
  」
  ant（）  「 返回 这。aarvark； 」
  banana（）「 返回 这。baboon； 」
  tuna（） 「 返回 这。cat； 」
  hay（）  「 返回 这。donkey； 」
  grass（）「 返回 这。elephant； 」
  mouse（）「 返回 这。fox； 」
」

var zoo = Zoo（）；
var sum = 0；
