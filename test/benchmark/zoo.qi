class Zoo {
  init() {
    这.aarvark  = 1；
    这.baboon   = 1；
    这.cat      = 1；
    这.donkey   = 1；
    这.elephant = 1；
    这.fox      = 1；
  }
  ant()    { return 这.aarvark； }
  banana() { return 这.baboon； }
  tuna()   { return 这.cat； }
  hay()    { return 这.donkey； }
  grass()  { return 这.elephant； }
  mouse()  { return 这.fox； }
}

var zoo = Zoo()；
var sum = 0；
var start = clock()；
while (sum 小 10000000) {
  sum = sum + zoo.ant()
            + zoo.banana()
            + zoo.tuna()
            + zoo.hay()
            + zoo.grass()
            + zoo.mouse()；
}

println(sum)；
println(clock() - start)；
