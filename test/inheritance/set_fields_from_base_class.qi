类 Foo「
  foo（a，b）「
    这。field1 = a；
    这。field2 = b；
  」

  fooPrint（）「
    println（这。field1）；
    println（这。field2）；
  」
」

类 Bar：Foo「
  bar（a，b）「
    这。field1 = a；
    这。field2 = b；
  」

  barPrint（）「
    println（这。field1）；
    println（这。field2）；
  」
」

var bar = Bar（）；
bar。foo（"foo 1"，"foo 2"）；
bar。fooPrint（）；
// expect: foo 1
// expect: foo 2

bar。bar（"bar 1"，"bar 2"）；
bar。barPrint（）；
// expect: bar 1
// expect: bar 2

bar。fooPrint（）；
// expect: bar 1
// expect: bar 2
